{
    "compilerOptions": {
        "module": "commonjs",
        "target": "es5",
        "lib": [
            "es6",
            "dom"
        ],
        "noImplicitAny": true,
        "noImplicitThis": true,
        "strictNullChecks": true,
        "baseUrl": "../",
        "noEmit": true,
        "forceConsistentCasingInFileNames": true
    },
    "include": [
        "index.d.ts",
        "test/**/*.ts"
    ]
}
